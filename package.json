{
   "name": "deepbackend",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/bcrypt": "^5.0.1",
      "@types/jsonwebtoken": "^9.0.4",
      "@types/multer": "^1.4.11",
      "@types/node": "^16.11.10",
      "nodemon": "^3.0.1",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@aws-sdk/client-s3": "^3.481.0",
      "@aws-sdk/s3-request-presigner": "^3.481.0",
      "@types/express": "^4.17.20",
      "bcrypt": "^5.1.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.6.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "sharp": "^0.32.1",
      "typeorm": "0.3.17"
   },
   "scripts": {
      "start": "tsc && ts-node-esm src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "server": "nodemon --exec ts-node-esm src/server.ts",
      "authserver": "nodemon --exec ts-node-esm src/authServer.ts",
      "migration:generate": "typeorm migration:create -n",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
   }
}
